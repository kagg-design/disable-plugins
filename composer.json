{
  "name": "KAGG/Disable_Plugins",
  "description": "MU-Plugin to disable some plugins under certain conditions",
  "license": "GPL-2.0-only",
  "homepage": "https://github.com/kagg-design/disable-plugins",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "KAGG Design",
      "email": "info@kagg.eu",
      "homepage": "https://kagg.eu/en/"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/OnTheGoSystems/patchwork.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "composer/installers": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "antecedent/patchwork": "^2.0",
    "sebastian/phpcpd": "^3.0",
    "10up/wp_mock": "~0.2",
    "phpunit/phpunit": "~5.7",
    "lucatume/function-mocker": "~1.3",
    "squizlabs/php_codesniffer": "^3.0",
    "phpcompatibility/php-compatibility": "^9.0",
    "wp-coding-standards/wpcs": "^2.0"
  },
  "autoload": {
    "classmap": [
      "includes"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "includes"
    ],
    "files": [
      "tests/phpunit/class-kagg-testcase.php"
    ]
  }
}
