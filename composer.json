{
  "name": "kagg/disable_plugins",
  "description": "MU-Plugin to disable plugins under certain conditions",
  "license": "GPL-2.0-only",
  "homepage": "https://github.com/kagg-design/disable-plugins",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "KAGG Design",
      "email": "info@kagg.eu",
      "homepage": "https://kagg.eu/en/"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "require": {
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "lucatume/function-mocker": "^1.3",
    "10up/wp_mock": "0.2 - 0.4",
    "phpunit/phpunit": "5.7 - 9.5",
    "squizlabs/php_codesniffer": "^3.6",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "wp-coding-standards/wpcs": "^2.3"
  },
  "autoload": {
    "classmap": [
      "includes"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/phpunit/classes",
      "includes"
    ]
  }
}
